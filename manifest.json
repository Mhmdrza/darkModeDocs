{
    "name": "DarkDocs",
    "version": "1.0.0",
    "description": "proper dark mode for google docs",
    "manifest_version": 3,
    "author": "MRA",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://docs.google.com/document/*"],
            "css": ["docs-dark.css"],
            "js": ["logic.js"]
        }
    ],
    "permissions": [
        "storage"
    ]
}