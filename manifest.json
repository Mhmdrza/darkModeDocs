{
    "name": "Snapp mail dark mode",
    "version": "1.0.1",
    "description": "dark mode for snapp mail",
    "manifest_version": 3,
    "author": "Mohammad reza Ala",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://mail.snapp.cab/*"],
            "css": ["styles.css"],
            "js": ["logic.js"]
        }
    ],
    "icons": {
        "16": "icon20.png",
        "32": "icon20.png",
        "48": "icon20.png",
        "120": "icon20.png"
    },
    "permissions": [
        "storage"
    ]
}